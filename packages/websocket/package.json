  },
  "homepage": "https://github.com/WindrunnerMax/FileTransfer",
  "dependencies": {
    "@arco-design/web-react": "2.56.1",
    "js-base64": "3.7.5",
    "laser-utils": "0.0.5-alpha.1",
    "pako": "2.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "4.7.2",
    "socket.io-client": "4.7.2",
    "@ft/webrtc": "workspace: *"
  },
