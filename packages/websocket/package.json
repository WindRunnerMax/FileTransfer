    "less": "3.0.0",
    "rimraf": "6.0.1",
    "less-loader": "6.0.0",
    "postcss": "8.3.3",
    "prettier": "2.4.1",
    "rollup": "2.75.7",
    "@rollup/plugin-node-resolve": "13.0.4",
